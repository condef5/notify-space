    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "microbundle": "^0.11.0",
    "prettier": "^1.17.1"
  },
